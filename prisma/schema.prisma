generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Schedule {
  id          String   @id @default(uuid())
  location    String
  address     String
  date        DateTime
  startTime   String
  endTime     String
  createdAt   DateTime @default(now())
}